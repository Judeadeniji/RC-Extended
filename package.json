{
  "name": "rc-extended",
  "version": "0.0.14",
  "description": "Cybernetically enhanced React components",
  "main": "dist/**/*",
  "types": "dist/**/*.ts",
  "type": "module",
  "exports": {
    "./store": {
      "types": "./dist/store/index.d.ts",
      "import": "./dist/store/index.js",
      "require": "./dist/store/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.js"
    },
    "./functions": {
      "types": "./dist/functions/index.d.ts",
      "import": "./dist/functions/index.js",
      "require": "./dist/functions/index.js"
    },
    "./use": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.js"
    }
  },
  "scripts": {
    "build": "npx tsc",
    "dev": "npx tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Judeadeniji/RC-Extended.git"
  },
  "keywords": [
    "react",
    "components",
    "rc",
    "extend",
    "custom components",
    "async"
  ],
  "author": "Adeniji Oluwaferanmi (TheLazyDev)",
  "bugs": {
    "url": "https://github.com/Judeadeniji/RC-Extended/issues"
  },
  "homepage": "https://github.com/Judeadeniji/RC-Extended#readme",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.21",
    "@types/webspeechapi": "^0.0.29",
    "@types/web-bluetooth": "^0.0.20",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "react": "^18.2.0"
  }
}
